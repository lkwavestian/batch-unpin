{
  "name": "batch-unpin",
  "displayName": "Batch Unpin Tabs",
  "description": "%extension.description%",
  "version": "0.0.1",
  "publisher": "lkwavestian",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "unpin",
    "tabs",
    "batch",
    "editor",
    "productivity"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lkwavestian/batch-unpin.git"
  },
  "bugs": {
    "url": "https://github.com/lkwavestian/batch-unpin/issues"
  },
  "homepage": "https://github.com/lkwavestian/batch-unpin#readme",
  "license": "MIT",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#2D3748",
    "theme": "dark"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "batch-unpin.unpinAllTabs",
        "title": "%command.unpinAllTabs.title%",
        "category": "Batch Unpin",
        "icon": {
          "light": "./images/unpin-dark.png",
          "dark": "./images/unpin-light.png"
        }
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "batch-unpin.unpinAllTabs",
          "when": "true"
        }
      ],
      "editor/context": [
        {
          "command": "batch-unpin.unpinAllTabs",
          "when": "true",
          "group": "1_modification"
        }
      ],
      "editor/title": [
        {
          "command": "batch-unpin.unpinAllTabs",
          "when": "true",
          "group": "navigation@1"
        }
      ],
      "statusBar/primary": [
        {
          "command": "batch-unpin.unpinAllTabs",
          "when": "true",
          "tooltip": "%command.unpinAllTabs.tooltip%"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "dev": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
